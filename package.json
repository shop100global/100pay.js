{
  "name": "@100pay-hq/100pay.js",
  "version": "1.5.0",
  "description": "100Pay.js is the official Nodejs API wrapper SDK that lets you easily verify crypto payments, run bulk payout, transfer assets and many more.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "start": "npm run build",
    "dev": "nodemon",
    "build": "tsup",
    "build:clean": "rm -rf dist && tsup",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "verify": "npm run lint && npm run build",
    "prepare": "npm run build",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major",
    "release:patch": "standard-version --release-as patch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shop100global/100pay.js.git"
  },
  "keywords": [
    "crypto",
    "payment",
    "100pay",
    "nodejs",
    "sdk",
    "100pay",
    "verify"
  ],
  "author": "brainyjosh",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/shop100global/100pay.js/issues"
  },
  "homepage": "https://github.com/shop100global/100pay.js#readme",
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@types/node": "^22.13.14",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "eslint": "^9.23.0",
    "globals": "^16.0.0",
    "nodemon": "^3.1.10",
    "standard-version": "^9.5.0",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "@untools/logger": "^0.0.6"
  }
}
